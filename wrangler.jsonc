{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "agent",
  "main": "worker/index.ts",
  "compatibility_date": "2025-02-04",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env"
  ],
  "assets": {
    "not_found_handling": "single-page-application",
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": true
  },
  // 1. The Super Agent Durable Object
  "durable_objects": {
    "bindings": [
      {
        "name": "SuperAgent",
        "class_name": "SuperAgent"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["SuperAgent"]
    }
  ],
  // 2. AI Binding
  "ai": {
    "binding": "AI"
  },
  // 3. Vectorize (Long-term Memory)
  "vectorize": [
    {
      "binding": "VECTOR_DB",
      "index_name": "ai-search-agent"
    }
  ],
"d1_databases": [
    {
      "binding": "DB",
      "database_name": "agent-db",
      "database_id": "959bb327-36ca-41f9-938a-7632d84957de"
    }
  ],
  // 4. R2 Bucket (File Sandbox)
  "r2_buckets": [
    {
      "binding": "FILES_BUCKET",
      "bucket_name": "agent-artifacts"
    }
  ],
  // 5. Browser Rendering (Web Browsing)
  "browser": {
    "binding": "BROWSER"
  },
  // 6. Deep Research Workflow
  "workflows": [
    {
      "name": "RESEARCH_WORKFLOW",
      "binding": "RESEARCH_WORKFLOW",
      "class_name": "ResearchWorkflow"
    }
  ]
}
